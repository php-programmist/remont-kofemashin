{% extends 'page_with_callback_form.html.twig' %}

    {% block title %}
        {% if(service.isService) %}
            {{ service.seoName }}  по низкой цене в Москве - «Ремонт Кофемашин»
        {% else %}
            {{ service.name }} {{ brand.name }} - «Ремонт Кофемашин»
        {% endif %}
    {% endblock %}

    {% block description %}
        {% if(service.isService) %}
            {{ service.seoName }}.
        {% else %}
            {{ service.seoName }}.
        {% endif %}
    {% endblock %}

    {% block description_price %}{{ service.price }}{% endblock %}

    {% block h1 %}
        {% if(service.isService) %}
            {{ service.seoName }}
        {% else %}
            {{ service.name }} {{ brand.name }}
        {% endif %}
    {% endblock %}
    {% block description_repair %}{{ service.isService == 0 ? 'ремонта' }}{% endblock %}

{% block content %}
    {{ include('modules/_service_content.html.twig') }}
{% endblock %}

